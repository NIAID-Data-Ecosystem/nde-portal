{
  "name": "nde-portal",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build:core": "yarn generate-footer-content && yarn generate-metadata-completeness-fields && next build",
    "build": "yarn with-prod-env yarn build:core",
    "build:staging": "env-cmd -f .env.staging yarn build:core",
    "build:dev": "env-cmd -f .env.dev  yarn build:core",
    "start": "next start",
    "lint": "next lint",
    "generate-footer-content": "tsx scripts/generate-footer-content.ts",
    "generate-metadata-completeness-fields": "tsx scripts/generate-metadata-completeness-fields.ts",
    "generate-schema-definitions": "yarn with-prod-env tsx scripts/generate-schema-definitions/index.ts",
    "generate-sitemaps": "yarn with-prod-env tsx scripts/generate-dataset-sitemaps.ts && yarn with-prod-env next-sitemap",
    "prepare": "husky install",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "with-prod-env": "env-cmd -f .env.production"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.10.7",
    "@emotion/react": "^11.11.3",
